pipeline{
	agent any
	environment {
		RELEASE = '1.3'
	}

	stages{
		stage('Build'){
			agent any
			environment {
				LOG_LEVEL = 'INFO'
			}
			steps{
				sh "echo Build - Release ${RELEASE} for LogLevel ${LOG_LEVEL}"
			}
		}
		stage('Test'){
			steps{
				sh "echo Testing - Release ${RELEASE} for LogLevel ${LOG_LEVEL}"
			}
		}
	}
}
