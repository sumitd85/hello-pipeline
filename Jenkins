pipeline{

agent any
environment{
  RELEASE= 1.3
}

stages{
  stage{
    agent any
    environment{
      LOG_LEVEL= INFO
    }
    stages{
      sh "echo Release ${RELEASE} for LogLevel ${LOG_LEVEL}"
    }
  }
  stage{
    stages{
      sh "echo Release ${RELEASE} for LogLevel ${LOG_LEVEL}"
    }
  }
}

}
